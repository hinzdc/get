<Configuration>
  <Add OfficeClientEdition="64" Channel="Current" MigrateArch="TRUE">
 
    <!-- Install Microsoft Visio Professional 2021 Retail -->
    <Product ID="VisioPro2021Retail">
      <Language ID="en-us" />
    </Product>
    
    <!-- Install Microsoft Project Professional 2021 Retail -->
    <Product ID="ProjectPro2021Retail">
      <Language ID="en-us" />
    </Product>
  
  </Add>

  <Property Name="FORCEAPPSHUTDOWN" Value="TRUE" />
  <Property Name="AUTOACTIVATE" Value="1" />
  <Updates Enabled="TRUE" />
  <RemoveMSI All="TRUE" />
  <Display Level="Full" AcceptEULA="TRUE" />

</Configuration>
